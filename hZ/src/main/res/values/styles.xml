<?xml version="1.0" encoding="utf-8"?>
<resources>
     <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <!-- Base style for animations.  This style specifies no animations. -->
    <style name="Animation" />

    <style name="Animation.TypingFilter">
        <item name="android:windowEnterAnimation">@anim/grow_fade_in_center</item>
        <item name="android:windowExitAnimation">@anim/shrink_fade_out_center</item>
    </style>
    <style name="Animation.TypingFilterRestore">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@anim/shrink_fade_out_center</item>
</style>
</resources>